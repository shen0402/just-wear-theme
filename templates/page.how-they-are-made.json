    "1660063827055e4625": {
      "type": "image-with-text",
      "blocks": {
        "1660063826f04c3aee-0": {
          "type": "heading",
          "settings": {
            "heading": "We recycle the wasted products for better use.",
            "heading_size": "h1"
          }
        },
        "1660063826f04c3aee-1": {
          "type": "text",
          "settings": {
            "text": "<p>Made from sustainable Austrian Beech Trees, our fabric is made from fibers which are <strong>40% cellulose<\/strong> and produced in a eco-responsible process that significantly reduces emissions and water impact. This process uses <strong>20x less water than cotton!<\/strong><\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "1660063826f04c3aee-0",
        "1660063826f04c3aee-1"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/wasted_cb3816a2-2f3f-4776-8d8a-334465710a3a.jpg",
